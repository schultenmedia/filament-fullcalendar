{
    "name": "schultenmedia/filament-fullcalendar",
    "description": "The Most Popular JavaScript Calendar integrated with Filament ðŸ’›",
    "keywords": [
        "schultenmedia",
        "laravel",
        "filament-fullcalendar"
    ],
    "homepage": "https://github.com/schultenmedia/filament-fullcalendar",
    "license": "MIT",
    "authors": [
        {
            "name": "SchultenMedia",
            "email": "schultenmedia@outlook.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "illuminate/contracts": "^10.0|^11.0",
        "spatie/laravel-package-tools": "^1.13.5"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.0|^8.0",
        "nunomaduro/larastan": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "SchultenMedia\\FilamentFullCalendar\\": "src"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SchultenMedia\\FilamentFullCalendar\\FilamentFullCalendarServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
